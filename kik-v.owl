        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.org/ozo/kik#hasQualityValue"/>
                <owl:someValuesFrom rdf:resource="https://purl.org/ozo/kik#DecubitusCategorieCodelijst"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.org/ozo/kik#heeftDatum"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="nl">Decubitusgraad</rdfs:label>
        <skos:definition xml:lang="nl">Beschrijving van de toestand van decubituswond op een bepaald tijdstip.
Meestal volgens de Decubituscategorie codelijst volgens de V&amp;VN Wondzorg definitie.</skos:definition>
        <vph:definitionSource xml:lang="nl">Gegevenswoordenboek</vph:definitionSource>
    </owl:Class>
    


    <!-- https://purl.org/ozo/kik#DecubitusWond -->

    <owl:Class rdf:about="https://purl.org/ozo/kik#DecubitusWond">
