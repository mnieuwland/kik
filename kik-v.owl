    <!-- http://purl.org/dc/terms/identifier -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/identifier">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
        <rdfs:label xml:lang="en">identifier</rdfs:label>
        <skos:definition xml:lang="nl">Een kenmerk dat als doel heeft het gekenmerkte (uniek) te identificeren.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/ozo/kik#ZiektePercentage -->

    <owl:DatatypeProperty rdf:about="https://purl.org/ozo/kik#ZiektePercentage">
        <rdfs:domain rdf:resource="https://purl.org/ozo/kik#ZiektePeriode"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label xml:lang="nl">Ziektepercentage</rdfs:label>
        <skos:definition xml:lang="nl">Percentage waarvoor iemand ziek is in een bepaalde Ziekteperiode.
Waarde is een geheel getal van 0[%] t/m 100[%].</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/ozo/kik#aoPercentage -->

    <owl:DatatypeProperty rdf:about="https://purl.org/ozo/kik#aoPercentage">
        <rdfs:domain rdf:resource="https://purl.org/ozo/kik#ArbeidsOngeschiktheid"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label xml:lang="nl">AO percentage</rdfs:label>
        <skos:definition xml:lang="nl">Het percentage waarvoor iemand niet in staat wordt geacht arbeid te verrichten.
De waarde is altijd een geheel getal en ligt tussen de 0[%] en 100[%].</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/ozo/kik#birthname -->

    <owl:DatatypeProperty rdf:about="https://purl.org/ozo/kik#birthname">
        <rdfs:domain rdf:resource="https://purl.org/ozo/vph#Human"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>geboortenaam</rdfs:label>
        <skos:definition xml:lang="nl">De geslachtsnaam waarmee iemand geboren is</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/ozo/kik#eindDatum -->

    <owl:DatatypeProperty rdf:about="https://purl.org/ozo/kik#eindDatum">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:label xml:lang="nl">einddatum</rdfs:label>
        <skos:closeMatch rdf:resource="https://www.w3.org/TR/owl-time/#time:hasEnd"/>
        <skos:definition xml:lang="nl">De datum waarop een interval eindigt.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/ozo/kik#gewerkteUren -->

    <owl:DatatypeProperty rdf:about="https://purl.org/ozo/kik#gewerkteUren">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:label xml:lang="nl">gewerkte uren</rdfs:label>
        <skos:definition xml:lang="nl">Aantal uren dat gewerkt is binnen een bepaalde periode</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/ozo/kik#hasNumericalValue -->

    <owl:DatatypeProperty rdf:about="https://purl.org/ozo/kik#hasNumericalValue">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:label xml:lang="nl">numerieke waarde</rdfs:label>
        <skos:definition xml:lang="nl">De numerieke waarde van een zekere uitkomst.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/ozo/kik#heeftBSN -->

    <owl:DatatypeProperty rdf:about="https://purl.org/ozo/kik#heeftBSN">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="nl">BSN</rdfs:label>
        <rdfs:label xml:lang="nl">Burger Service Nummer</rdfs:label>
        <skos:definition xml:lang="nl">Het nummer waarmee ieder Nederlandse burger uniek geïdentificeerd kan worden.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/ozo/kik#heeftDatum -->

    <owl:DatatypeProperty rdf:about="https://purl.org/ozo/kik#heeftDatum">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:label xml:lang="nl">heeft datum</rdfs:label>
        <skos:definition xml:lang="nl">een datum die kenmerkend is</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/ozo/kik#heeftHuisletter -->

    <owl:DatatypeProperty rdf:about="https://purl.org/ozo/kik#heeftHuisletter">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://purl.org/ozo/kik#NummerAanduiding"/>
        <rdfs:label xml:lang="nl">huisletter</rdfs:label>
        <skos:definition xml:lang="nl">Een huisletter is een authentiek gegeven met betrekking tot een nummeraanduiding. Het is de lettering van een adresseerbaar object</skos:definition>
        <skos:exactMatch rdf:resource="http://bag.basisregistraties.overheid.nl/def/bag#huisletter"/>
        <vph:definitionSource rdf:resource="https://bag.basisregistraties.overheid.nl/begrippenlijst"/>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/ozo/kik#heeftHuisnummer -->

    <owl:DatatypeProperty rdf:about="https://purl.org/ozo/kik#heeftHuisnummer">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://purl.org/ozo/kik#NummerAanduiding"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label xml:lang="nl">huisnummer</rdfs:label>
        <skos:definition xml:lang="nl">Een huisnummer is een authentiek gegeven met betrekking tot een nummeraanduiding. Het is de nummering van een adresseerbaar object.</skos:definition>
        <skos:exactMatch rdf:resource="http://bag.basisregistraties.overheid.nl/def/bag#huisnummer"/>
        <vph:definitionSource rdf:resource="https://bag.basisregistraties.overheid.nl/begrippenlijst"/>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/ozo/kik#heeftHuisnummerToevoeging -->

    <owl:DatatypeProperty rdf:about="https://purl.org/ozo/kik#heeftHuisnummerToevoeging">
        <rdfs:domain rdf:resource="https://purl.org/ozo/kik#NummerAanduiding"/>
        <rdfs:label xml:lang="nl">huisnummertoevoeging</rdfs:label>
        <skos:definition xml:lang="nl">Een huisnummertoevoeging is een authentiek gegeven met betrekking tot een nummeraanduiding. Het is de toevoeging aan de nummering van een adresseerbaar object.</skos:definition>
        <skos:exactMatch rdf:resource="http://bag.basisregistraties.overheid.nl/def/bag#huisnummertoevoeging"/>
        <vph:definitionSource rdf:resource="https://bag.basisregistraties.overheid.nl/begrippenlijst"/>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/ozo/kik#heeftKvkNummer -->

    <owl:DatatypeProperty rdf:about="https://purl.org/ozo/kik#heeftKvkNummer">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
