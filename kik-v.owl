    <!-- https://purl.org/ozo/vph#hasRole -->

    <rdf:Description rdf:about="https://purl.org/ozo/vph#hasRole">
        <rdfs:label xml:lang="nl">heeft rol</rdfs:label>
        <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/BFO_0000023"/>
        <skos:definition xml:lang="nl">Verwijst naar de rol van een subject die op een bepaald moment in de tijd geldig is.</skos:definition>
    </rdf:Description>
    


    <!-- https://purl.org/ozo/kik#agreementOf -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#agreementOf">
        <owl:inverseOf rdf:resource="https://purl.org/ozo/kik#hasAgreement"/>
        <rdfs:domain rdf:resource="https://purl.org/ozo/vph#Agreement"/>
        <rdfs:label xml:lang="nl">overeenkomst van</rdfs:label>
        <skos:definition xml:lang="nl">Geeft van een overeenkomst aan wie de eigenaar is van die overeenkomst.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#bijbehorendeOpenbareRuimte -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#bijbehorendeOpenbareRuimte">
        <rdfs:range rdf:resource="https://purl.org/ozo/vph#PublicArea"/>
        <rdfs:label xml:lang="nl">bijbehorende openbare ruimte</rdfs:label>
        <skos:exactMatch rdf:resource="http://bag.basisregistraties.overheid.nl/def/bag#bijbehorendeWoonplaats"/>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#bijbehorendeWoonplaats -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#bijbehorendeWoonplaats">
        <rdfs:range rdf:resource="https://purl.org/ozo/vph#VillageOrCity"/>
        <rdfs:label xml:lang="nl">bijbehorende woonplaats</rdfs:label>
        <skos:exactMatch rdf:resource="https://bag.basisregistraties.overheid.nl/datamodel#bijbehorendeWoonplaats"/>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#hasAgreement -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#hasAgreement">
        <rdfs:range rdf:resource="https://purl.org/ozo/vph#Agreement"/>
        <rdfs:label xml:lang="nl">heeft overeenkomst</rdfs:label>
        <skos:definition xml:lang="nl">Legt een relatie tussen de eigenaar van een overeenkomst en die overeenkomst.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#hasDenominatorQualityValue -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#hasDenominatorQualityValue">
        <rdfs:subPropertyOf rdf:resource="https://purl.org/ozo/kik#hasQualityValue"/>
        <rdfs:label xml:lang="nl">heeft noemer kwaliteitswaarde</rdfs:label>
        <skos:definition xml:lang="nl">Verwijst specifiek naar de waarde van een kwaliteitsmeting die de noemer vormt van die meting.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#hasDescription -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#hasDescription">
        <rdfs:range rdf:resource="https://purl.org/ozo/vph#Description"/>
        <rdfs:label xml:lang="nl">heeft beschrijving</rdfs:label>
        <skos:definition xml:lang="nl">Legt een relatie tussen een instantie en een beschrijving van die instantie</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#hasInput -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#hasInput">
        <rdfs:label xml:lang="nl">heeft input</rdfs:label>
        <skos:definition xml:lang="nl">Geeft de input aan. 
Voorbeeld: een WLZ indicatie vormt de input voor het zorgproces van een client.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#hasMember -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#hasMember">
        <rdfs:label xml:lang="nl">heeft lid</rdfs:label>
        <skos:definition xml:lang="nl">Verwijst naar het lid van een verzameling.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#hasNumeratorQualityValue -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#hasNumeratorQualityValue">
        <rdfs:subPropertyOf rdf:resource="https://purl.org/ozo/kik#hasQualityValue"/>
        <rdfs:label xml:lang="nl">heeft teller kwaliteitswaarde</rdfs:label>
        <skos:definition xml:lang="nl">Verwijst specifiek naar de waarde van een kwaliteitsmeting die de teller vormt van die meting.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#hasOutput -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#hasOutput">
        <rdfs:label xml:lang="nl">heeft output</rdfs:label>
        <skos:definition xml:lang="nl">Verwijst naar hetgeen voortkomt uit het subject.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#hasPart -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#hasPart">
        <owl:inverseOf rdf:resource="https://purl.org/ozo/kik#partOf"/>
        <rdfs:isDefinedBy rdf:resource="http://www.ontobee.org/ontology/RO"/>
        <rdfs:label xml:lang="nl">heeft onderdeel</rdfs:label>
        <skos:definition xml:lang="nl">Een kernrelatie die geldt tussen een geheel en een onderdeel daarvan.</skos:definition>
        <skos:exactMatch rdf:resource="http://purl.obolibrary.org/obo/BFO_0000051"/>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#hasParticipant -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#hasParticipant">
        <owl:inverseOf rdf:resource="https://purl.org/ozo/kik#participatesIn"/>
        <rdfs:label xml:lang="nl">heeft deelnemer</rdfs:label>
        <skos:definition xml:lang="nl">Een relatie tussen een proces (perdurant) en een endurant die op enige manier betrokken is in dat proces.</skos:definition>
        <skos:exactMatch rdf:resource="http://purl.obolibrary.org/obo/RO_0000057"/>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#hasQuality -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#hasQuality">
        <owl:inverseOf rdf:resource="https://purl.org/ozo/kik#qualityOf"/>
        <rdfs:domain rdf:resource="https://purl.org/ozo/vph#SpatioTemporalParticular"/>
        <rdfs:range rdf:resource="https://purl.org/ozo/vph#Quality"/>
        <rdfs:label xml:lang="nl">heeft kwaliteit</rdfs:label>
        <skos:definition xml:lang="nl">Verwijst naar een kwaliteit.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#hasQualityValue -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#hasQualityValue">
        <rdfs:range rdf:resource="https://purl.org/ozo/vph#QualityValue"/>
        <rdfs:label xml:lang="nl">heeft kwaliteitswaarde</rdfs:label>
        <skos:definition xml:lang="nl">Verwijst naar de waarde van een kwaliteitsmeting.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#hasSubject -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#hasSubject">
        <rdfs:label xml:lang="nl">heeft onderwerp</rdfs:label>
        <skos:definition xml:lang="nl">Verwijst naar het onderwerp waarover iets gaat. 
Voorbeeld: een zorgdossier heeft_onderwerp client waarover het dossier gaat.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#hasUnitOfMeasure -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#hasUnitOfMeasure">
        <rdfs:range rdf:resource="https://purl.org/ozo/vph#UnitOfMeasure"/>
        <rdfs:label xml:lang="nl">Verwijst naar de meeteenheid van een meting.</rdfs:label>
        <rdfs:label xml:lang="nl">heeft meeteenheid</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#heeftVerzuimPeriode -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#heeftVerzuimPeriode">
        <rdfs:domain rdf:resource="https://purl.org/ozo/kik#WerkOvereenkomst"/>
        <rdfs:range rdf:resource="https://purl.org/ozo/kik#VerzuimPeriode"/>
        <rdfs:label xml:lang="nl">heeft verzuimperiode</rdfs:label>
        <skos:definition xml:lang="nl">Verwijst naar de verzuimperiode die hoort bij een werkovereenkomst.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#is_client_binnen_verpleegproces -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#is_client_binnen_verpleegproces">
        <rdfs:subPropertyOf rdf:resource="https://purl.org/ozo/kik#participatesIn"/>
        <rdfs:domain rdf:resource="https://purl.org/ozo/vph#Human"/>
        <rdfs:range rdf:resource="https://purl.org/ozo/kik#NursingProcess"/>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#locatedIn -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#locatedIn">
        <owl:inverseOf rdf:resource="https://purl.org/ozo/kik#locationOf"/>
        <rdfs:label xml:lang="nl">heeft lokatie</rdfs:label>
        <skos:altLabel xml:lang="en">A relation between two independent continuants, the target and the location, in which the target is entirely within the location</skos:altLabel>
        <skos:definition xml:lang="nl">Een relatie tussen twee perdurants, het subject en de locatie waarin het subject zich geheel bevindt.</skos:definition>
        <skos:exactMatch rdf:resource="http://purl.obolibrary.org/obo/RO_0001025"/>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#locationOf -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#locationOf">
        <rdfs:label xml:lang="nl">lokatie van</rdfs:label>
        <skos:definition xml:lang="nl">een relatie tussen twee endurants, een lokatie en een subject, waarbij het subject zich geheel bevindt binnen die lokatie.</skos:definition>
        <skos:exactMatch rdf:resource="http://purl.obolibrary.org/obo/RO_0001015"/>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#partOf -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#partOf">
        <rdfs:label xml:lang="nl">onderdeel van</rdfs:label>
        <skos:definition xml:lang="nl">Een kernrelatie die geldt tussen een deel het geheel waartoe dat deel behoort.</skos:definition>
        <skos:exactMatch rdf:resource="http://purl.obolibrary.org/obo/BFO_0000050"/>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#participatesIn -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#participatesIn">
        <rdfs:label xml:lang="nl">neemt deel in</rdfs:label>
        <skos:definition xml:lang="nl">Een relatie tussen een endurant en een proces (perdurant) waarbij de endurant op enige manier betrokken is in dat proces.</skos:definition>
        <skos:exactMatch rdf:resource="http://purl.obolibrary.org/obo/RO_0000056"/>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#qualityOf -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#qualityOf">
        <rdfs:domain rdf:resource="https://purl.org/ozo/vph#Quality"/>
        <rdfs:label xml:lang="nl">kwaliteit van</rdfs:label>
        <skos:definition xml:lang="nl">Verwijst naar de houder van een zekere kwaliteit.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ozo/kik#roleOf -->

    <owl:ObjectProperty rdf:about="https://purl.org/ozo/kik#roleOf">
        <rdfs:label xml:lang="nl">rol van</rdfs:label>
        <skos:definition xml:lang="nl">Geeft van een rol aan wie die rol vervult.</skos:definition>
    </owl:ObjectProperty>
    


