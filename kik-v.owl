            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="nl">Ziekteperiode</rdfs:label>
        <skos:definition xml:lang="nl">Een ziekte priode is een verzuimperiode met ziekte als oorzaak van het verzuim.</skos:definition>
    </owl:Class>
    


    <!-- https://purl.org/ozo/kik#Zorgdossier -->

    <owl:Class rdf:about="https://purl.org/ozo/kik#Zorgdossier">
        <rdfs:subClassOf rdf:resource="https://purl.org/ozo/vph#Description"/>
        <rdfs:label xml:lang="nl">Zorgdossier</rdfs:label>
