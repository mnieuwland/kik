        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.org/ozo/kik#hasQuality"/>
                <owl:someValuesFrom rdf:resource="https://purl.org/ozo/kik#DecubitusGraad"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.org/ozo/kik#partOf"/>
                <owl:someValuesFrom rdf:resource="https://purl.org/ozo/vph#Human"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:isDefinedBy xml:lang="nl">SNOMED CT</rdfs:isDefinedBy>
        <rdfs:label xml:lang="nl">Decubituswond</rdfs:label>
        <skos:definition xml:lang="nl">Een gelokaliseerde beschadiging van de huid en/of onderliggend weefsel, meestal ter hoogte van een botuitsteeksel, als gevolg van druk of van druk in samenhang met schuifkracht.</skos:definition>
        <vph:definitionSource rdf:resource="https://terminologie.nictiz.nl/terminology/snomed/viewConcept/399912005"/>
    </owl:Class>
    


    <!-- https://purl.org/ozo/kik#EQFKwalificatie -->

