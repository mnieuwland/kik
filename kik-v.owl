        <rdfs:label xml:lang="nl">EQF Kwalificatiewaarde</rdfs:label>
        <skos:definition xml:lang="nl">Een kwalificatiewaarde volgens het Europees Kwalificatieraamwerk.</skos:definition>
    </owl:Class>
    


