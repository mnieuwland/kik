        <rdfs:label xml:lang="nl">Decubituscategorie codelijst</rdfs:label>
        <skos:definition xml:lang="nl">Lijst van categorieën om de ernst van een decubitus wond te categoriseren.</skos:definition>
        <skos:exactMatch rdf:resource="https://decor.nictiz.nl/art-decor/decor-valuesets--venvn-wond-?id=2.16.840.1.113883.2.4.3.11.60.40.2.19.1.1&amp;effectiveDate=2017-12-31T00:00:00&amp;language=nl-NL"/>
        <vph:definitionSource xml:lang="nl">V&amp;VN Wondzorg</vph:definitionSource>
    </owl:Class>
    


    <!-- https://purl.org/ozo/kik#DecubitusGraad -->

    <owl:Class rdf:about="https://purl.org/ozo/kik#DecubitusGraad">
